<div class="content_container main_container">
    <h4 class="promo_main_header">Shuttle</h3>
    <div class="contact_container">
        <div class="row">
            <div class="col-md-8 contact_contents">
                <img src="//codecloud.cdn.speedyrails.net/sites/5954085c6e6f64023b850000/image/jpeg/1498754954000/shuttle.jpg" class="" alt="">
                <div id="mm_page_content" class="promo_list_desc"></div>
            </div>
            <div class="col-md-4 hidden_phone">
            </div> 
        </div>
    </div>
</div>
<script>
    $(document).ready(function(e){
        init(e);
        loadMallDataCached(renderPage);
    })
    
    function renderPage(){
        var pathArray = window.location.pathname.split( '/' );
        var slug = pathArray[pathArray.length-1];
        
        prefix = get_prefix();
        $('#top_banner').addClass(slug)
        var pages_json = prefix + "/pages/fairlakes-shuttle.json"
        $.getJSON(pages_json).done(function(data) {
            $('#mm_page_content').html(data.body);
        }).fail(function(jqXHR) {
            if (jqXHR.status == 404) {
                $("#mm_page_content").text("Sorry. We cant find the page you looking for.");
            }
        }); 
        show_content();
    }
</script>